{
  "name": "news-channel",
  "version": "0.0.1",
  "description": "新闻频道，GraphQL、Next.js、Prisma",
  "main": "index.js",
  "repository": "https://github.com/onlyling/news-channel.git",
  "author": "LING_ZI_QING <onlylingstudio@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "next",
    "build": "yarn build:nexus-typegen && next build",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only src/schema",
    "start": "next start",
    "ncu": "npx npm-check-updates -u",
    "commit": "npx git-cz",
    "lint:ts": "tsc --noEmit && eslint src --ext .ts,.tsx",
    "gen:gql": "fruits-chain-gc http://localhost:3000/api/graphql --no-ignore-eslint --scalars BigDecimal:number ID:string Date:string DateTime:string",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@apollo/client": "^3.7.10",
    "@apollo/server": "^4.5.0",
    "@as-integrations/next": "^1.2.0",
    "@hapi/iron": "^7.0.1",
    "@prisma/client": "^4.11.0",
    "ahooks": "^3.7.5",
    "antd": "^5.3.1",
    "cookie": "^0.5.0",
    "deepmerge": "^4.3.1",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "next": "^13.2.4",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.4",
    "prisma": "^4.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@fruits-chain/eslint-config-preset": "^0.2.16",
    "@fruits-chain/graphql-codegen-preset": "^0.2.1",
    "@fruits-chain/prettier-config-preset": "^0.0.3",
    "@types/cookie": "^0.5.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "prettier": "@fruits-chain/prettier-config-preset",
  "resolutions": {
    "prettier": "^2.5.1"
  }
}
