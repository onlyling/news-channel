{
  "name": "news-channel",
  "version": "0.0.1",
  "description": "新闻频道，GraphQL、Next.js、Prisma",
  "main": "index.js",
  "repository": "https://github.com/onlyling/news-channel.git",
  "author": "LING_ZI_QING <onlylingstudio@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "next",
    "build": "yarn build:nexus-typegen && next build",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only src/schema",
    "start": "next start",
    "ncu": "npx npm-check-updates -u",
    "commit": "npx git-cz",
    "prisma:gen": "prisma generate"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@as-integrations/next": "^1.2.0",
    "@prisma/client": "^4.10.1",
    "graphql": "^16.6.0",
    "next": "^13.2.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.4",
    "prisma": "^4.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@fruits-chain/eslint-config-preset": "^0.2.14",
    "@fruits-chain/prettier-config-preset": "^0.0.3",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "prettier": "@fruits-chain/prettier-config-preset",
  "resolutions": {
    "prettier": "^2.5.1"
  }
}
